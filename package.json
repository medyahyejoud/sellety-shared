{
  "name": "sellety-shared",
  "license": "MIT",
  "version": "0.1.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./shopify": {
      "import": "./dist/shopify.mjs",
      "require": "./dist/shopify.js"
    },
    "./sanity": {
      "import": "./dist/sanity.mjs",
      "require": "./dist/sanity.js"
    }
  },
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "build": "tsup src/index.ts src/shopify.ts src/sanity.ts --format cjs,esm --dts",
    "lint": "tsc",
    "ci": "pnpm lint && pnpm test && pnpm build",
    "release": "pnpm lint && pnpm test && pnpm build && changeset publish"
  },
  "peerDependencies": {
    "graphql-request": "^6.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/node": "^20.8.7",
    "graphql-request": "^6.1.0",
    "tsup": "^7.2.0",
    "type-fest": "^4.5.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "zod": "^3.22.4"
  }
}
